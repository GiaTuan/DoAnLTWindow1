<Window x:Class="WpfApp2.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="SavePreset" Height="150" Width="320">
    <Canvas Margin="0,0,0.333,-19.333">
        <TextBlock Text="Name Preset:" Canvas.Top="29" Height="18" Canvas.Left="10"/>
        <TextBox Width="181" Canvas.Left="102" Canvas.Top="29" Name="NameTextBox"></TextBox>
        <Button Width="80" Height="25" Content="OK" Canvas.Left="102" Canvas.Top="69" Background="WhiteSmoke" Name="OKButton" Click="Button_Click"></Button>
        <Button Width="80" Height="24" Content="Cancel" Canvas.Left="203" Canvas.Top="70" Background="WhiteSmoke" Name="CancelButton" Click="Button_Click_1"></Button>
    </Canvas>
</Window>
